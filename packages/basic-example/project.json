{
  "name": "basic-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/basic-example/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/basic-example",
        "main": "packages/basic-example/src/main.ts",
        "tsConfig": "packages/basic-example/tsconfig.app.json",
        "assets": ["packages/basic-example/src/api"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "basic-example:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "basic-example:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/basic-example/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/basic-example/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
