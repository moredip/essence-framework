{
  "name": "essence",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/essence/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "outputPath": "dist/packages/essence",
        "main": "packages/essence/src/main.ts",
        "tsConfig": "packages/essence/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "essence:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "essence:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/essence/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/essence/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
